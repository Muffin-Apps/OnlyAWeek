<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.muffinapps.onlyaweek"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:name="org.muffinapps.onlyaweek.OnlyAWeekApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.muffinapps.onlyaweek.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.muffinapps.onlyaweek.AddNewExamActivity"
            android:label="@string/title_activity_add" >
        </activity>
        <activity
            android:name="org.muffinapps.onlyaweek.PlanningActivity"
            android:label="@string/title_activity_planning" >
        </activity>
    </application>

</manifest>
